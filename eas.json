{
  "build": {
    "preview": {
      "android": {
        "buildType": "apk",
        "image": "latest"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle",
        "autoIncrement": true,
        "image": "latest",
        "env": {
          "ANDROID_COMPILE_SDK_VERSION": "34",
          "ANDROID_TARGET_SDK_VERSION": "34"
        }
      }
    }
  },
  "cli": {
    "appVersionSource": "remote"
  }
}
